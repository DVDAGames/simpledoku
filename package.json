{
  "name": "simpledoku",
  "version": "0.2.0",
  "description": "Electron app sudoku game built on React",
  "keywords": [
    "sudoku",
    "electron",
    "game"
  ],
  "homepage": "https://github.com/ericrallen/simpledoku",
  "bugs": "https://github.com/ericrallen/simpledoku/issues",
  "license": "MIT",
  "author": {
    "name": "Eric Allen",
    "url": "https://www.interwebalchemy.com/"
  },
  "main": "app/window.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericrallen/simpledoku"
  },
  "scripts": {
    "clean": "cleandir ./app/",
    "copy:main": "cp ./src/window.js ./app/window.js",
    "copy:html": "cp ./src/index.html ./app/index.html",
    "copy:services": "rm -rf ./app/services && mkdir ./app/services && cp ./src/services/puzzle.js ./app/services/puzzle.js",
    "copy:templates": "cp ./src/templates/package.json ./app/package.json",
    "electron": "electron .",
    "server": "node ./app/server.js",
    "watch": "webpack --watch",
    "eslint": "npm run lint",
    "build": "npm run clean && webpack -p",
    "generate": "npm run copy:main & npm run copy:html & npm run copy:services & npm run copy:templates",
    "start": "npm run copy:main & npm run copy:html & npm run copy:services & npm run copy:templates && npm run electron",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^2.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "clean-dir": "^1.0.3",
    "copy": "^0.2.0",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^0.37.2",
    "eslint": "^2.7.0",
    "eslint-config-es2015-react": "^0.1.2",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "hapi": "^13.2.2",
    "inert": "^3.2.0",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1"
  },
  "engines": {
    "node": ">=5.x.x"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": "eslint-config-es2015-react/base",
    "rules": {
      "comma-dangle": [2, "never"]
    }
  }
}
