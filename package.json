{
  "name": "simpledoku",
  "version": "0.0.1",
  "description": "Electron app sudoku game built on React",
  "main": "dist/window.js",
  "scripts": {
    "clean": "cleandir ./dist/",
    "webpack": "webpack",
    "copy:window": "cp ./src/window.js ./dist/window.js",
    "copy:html": "cp ./src/index.html ./dist/index.html",
    "copy:server": "cp ./src/server.js ./dist/server.js",
    "electron": "electron .",
    "electron:rebuild": "electron-rebuild .",
    "server": "node ./dist/server.js",
    "watch": "npm run webpack",
    "start": "npm run copy:window & npm run copy:html && npm run electron",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^2.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "clean-dir": "^1.0.3",
    "copy": "^0.2.0",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^0.37.2",
    "electron-rebuild": "^1.1.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "hapi": "^13.2.2",
    "inert": "^3.2.0",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1"
  }
}
